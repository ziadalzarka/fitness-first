{
  "name": "fitness-first",
  "description": "Fitness First GYM Management",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "rimraf dist && electron-builder build --windows --ia32"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "gray",
    "angular",
    "fitness",
    "gym"
  ],
  "build": {
    "files": [
      "main.js",
      {
        "from": "src",
        "to": "src",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "output": "dist"
    }
  },
  "author": "Ziad Alzarka",
  "license": "LICENSE.md",
  "devDependencies": {
    "electron": "^4.1.3",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "angular": "^1.7.8",
    "angular-i18n": "^1.7.8",
    "angular-ui-bootstrap": "^2.5.6",
    "bootflat": "^2.0.4",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "lowdb": "^1.0.0",
    "moment": "^2.24.0",
    "popper.js": "^1.15.0"
  }
}
